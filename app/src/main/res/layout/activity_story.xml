<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/Sully"
    tools:context=".story">

    <TextView
        android:id="@+id/textView"
        android:layout_width="177dp"
        android:layout_height="40dp"
        android:fontFamily="@font/monsterfont"
        android:text="Historia"
        android:textColor="#006ED0"
        android:textSize="48sp"
        tools:layout_editor_absoluteX="117dp"
        tools:layout_editor_absoluteY="72dp" />

    <View
        android:id="@+id/divider3"
        android:layout_width="409dp"
        android:layout_height="70dp"
        android:background="?android:attr/listDivider"
        android:backgroundTint="@color/roxo"
        android:backgroundTintMode="add"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="661dp" />

    <View
        android:id="@+id/divider6"
        android:layout_width="425dp"
        android:layout_height="52dp"
        android:background="?android:attr/listDivider"
        android:backgroundTint="#B48CFC"
        android:backgroundTintMode="src_over"
        tools:layout_editor_absoluteX="-7dp"
        tools:layout_editor_absoluteY="1dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="306dp"
        android:layout_height="289dp"
        android:text="TextView"
        tools:layout_editor_absoluteX="52dp"
        tools:layout_editor_absoluteY="136dp" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="415dp"
        android:layout_height="209dp"
        android:scaleType="fitCenter"
        app:srcCompat="@drawable/cambada"
        tools:layout_editor_absoluteX="-2dp"
        tools:layout_editor_absoluteY="434dp" />
</androidx.constraintlayout.widget.ConstraintLayout>